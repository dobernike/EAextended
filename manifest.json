{
  "manifest_version": 2,
  "name": "EAextend",
  "version": "2.0",
  "description": "Автоматизация накладной",
  "icons": {
    "48": "img/icon.png"
  },

  "browser_action": {
    "default_title": "Автоматизация накладной",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "storage",
    "https://euroauto.ru/*",
    "http://euroauto.ru/*",
    "http://localhost/*"
  ],

  "content_scripts": [{
      "matches": ["http://euroauto.ru/raschet_stoimosti_rabot/*",
        "https://euroauto.ru/raschet_stoimosti_rabot/*"
      ],
      "js": ["workPrice.js"],
      "run_at": "document_end"
    },
    // Новые
    {
      "matches": ["https://amtel.club/cabinet/history/lc?date_start=11.06.2018&date_end=18.06.2018&status=all*"],
      "js": ["historyOpt.js"],
      "run_at": "document_end"
    }
  ]
}